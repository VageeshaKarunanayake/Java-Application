            <AuxValue name="JavaCodeGenerator_CreateCodePost" type="java.lang.String" value="AutoCompleteDecorator.decorate(vehicle_plate_input);&#xd;&#xa;vehicle_plate_input.addItem(&quot;&quot;);&#xd;&#xa;try {&#xd;&#xa;Connection conn = DatabaseConnection.connect();&#xd;&#xa;PreparedStatement DBOUTPUT = conn.prepareStatement(&quot;SELECT * FROM carDetails&quot;);&#xd;&#xa;ResultSet RS = DBOUTPUT.executeQuery();&#xd;&#xa;while(RS.next()){ &#xd;&#xa;            if(RS.getDate(&quot;departure_date_time&quot;) == null)&#xd;&#xa;                vehicle_plate_input.addItem(RS.getString(&quot;vehicle_plate&quot;));&#xd;&#xa;&#x9;&#x9;} &#xd;&#xa;DBOUTPUT.close();&#xd;&#xa;RS.close();&#xd;&#xa;conn.close();&#xd;&#xa;}catch(Exception e) {&#xd;&#xa;&#x9;&#x9;System.out.println(e);&#xd;&#xa;&#x9;}&#xd;&#xa;&#xd;&#xa;vehicle_plate_input.setSelectedIndex(0);&#xd;&#xa;&#xd;&#xa;vehicle_plate_input.addItemListener(new ItemListener() {&#xd;&#xa;            &#xd;&#xa;            public void itemStateChanged(ItemEvent event) {&#xd;&#xa;                &#xd;&#xa;                if (event.getStateChange() == ItemEvent.SELECTED) {&#xd;&#xa;                    String txt = vehicle_plate_input.getSelectedItem().toString();&#xd;&#xa;                    try {&#xd;&#xa;                    Connection conn = DatabaseConnection.connect();&#xd;&#xa;                    PreparedStatement DBOUTPUT2 = conn.prepareStatement(&quot;SELECT vehicle_type, arrival_date_time FROM carDetails WHERE vehicle_plate = ?&quot;);&#xd;&#xa;                    DBOUTPUT2.setString(1, txt);&#xd;&#xa;                    ResultSet RS2 = DBOUTPUT2.executeQuery();&#xd;&#xa;&#xd;&#xa;                    if(RS2.next()){ &#xd;&#xa;                                    vehicle_type_display.setText(RS2.getString(&quot;vehicle_type&quot;));&#xd;&#xa;                                    Date date = RS2.getDate(&quot;arrival_date_time&quot;);&#xd;&#xa;                                    DateFormat dateFormat = new SimpleDateFormat(&quot;yyyy-MM-dd hh:mm a&quot;);  &#xd;&#xa;                                    String SAD = dateFormat.format(date);&#xd;&#xa;                                    arrival_date_time_display.setText(SAD);&#xd;&#xa;                                    &#xd;&#xa;                    }&#xd;&#xa;                    DBOUTPUT2.close();&#xd;&#xa;                    RS2.close();&#xd;&#xa;                    conn.close();&#xd;&#xa;                }catch(Exception e) {&#xd;&#xa;                            System.out.println(e);&#xd;&#xa;                                    }&#xd;&#xa;            }&#xd;&#xa;           }&#xd;&#xa;        });"/>
